<?xml version="1.0"?>
<package>
  <metadata>
    <id>HBD.Singleton</id>
    <version>${NugetVersion}</version>
    <title>HBD.Singleton</title>
    <authors>Duy Hoang</authors>
    <owners>Duy Hoang</owners>
    <description>
      This library is separated from HBD.Framework, allows to manage singleton instances, load and re-load from Lazy functions.
    </description>
    <releaseNotes>
      Version ${NugetVersion}
      - Moved from HBD.Framework.
      - Support MemoryCache for all .Net Framework
    </releaseNotes>
    <summary>
      The helpful library for .Net developers which support:
      - Singleton Service.
    </summary>
    <language>en-US</language>
    <projectUrl>https://github.com/baoduy/HBD.Services</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/baoduy/Images/master/NugetLogo.PNG</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://opensource.org/licenses/MIT</licenseUrl>
    <copyright>Copyright 2017</copyright>
    <dependencies>
      <group targetFramework=".NETFramework4.5.2">
      </group>
      <group targetFramework=".NETStandard1.6">
        <dependency id="NETStandard.Library" version="1.6.1" />
      </group>
      <group targetFramework="NETStandard2.0">
        <dependency id="NETStandard.Library" version="2.0.0" />
      </group>
    </dependencies>
    <references></references>
    <tags>Framework Adapters Extensions</tags>
  </metadata>
  <files>
    <file src="HBD.Services.Singleton.4x\bin\Release\HBD.Services.*" target="lib\net452" exclude="**\*.pdb"/>
    <file src="HBD.Services.Singleton.St16\bin\Release\netstandard1.6\HBD.Services.*" target="lib\netstandard1.6" exclude="**\*.pdb"/>
    <file src="HBD.Services.Singleton.St20\bin\Release\netstandard2.0\HBD.Services.*" target="lib\netstandard2.0" exclude="**\*.pdb"/>
  </files>
</package>